
<%= render 'list', book: @book, user: User.find(current_user.id) %>
  <h1>Books</h1>
  <th>Title</th>
    <th>Opinion</th>
<% @books.each do |post_image| %>
  <div>
   <p><%= image_tag post_image.user.get_profile_image(100,100) %></p>
  <%= link_to book_path(post_image.id) do %>
    <p><%= post_image.title %></p>
  <% end %>
    <p><%= post_image.body%></p>

<% end %>
