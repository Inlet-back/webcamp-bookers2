<h2>Editing Book</h2>
<%= form_with model: @book do |f| %>
  <p></p><label for="Title">Title</label>
  <%= f.text_field :title, autofocus: true, id:"inputName", placeholder:"名前"%>
  </p>

  <p><label for="Body">Opinion</label>
  <%= f.text_field :body, autofocus: true, id:"inputName", placeholder:"名前"%>
</p>
  <%= f.submit "Update Book" %>
<% end %>
        <%= link_to "Show", book_path(@book.id)%>
        <%= link_to "Back", books_path%>